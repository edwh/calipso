{
  "name": "unified-calendar",
  "version": "0.1.0",
  "description": "Unified calendar Chrome extension with privacy-safe meeting detection",
  "type": "module",
  "scripts": {
    "build": "parcel build src/manifest.json --config @parcel/config-webextension --no-cache --public-url /",
    "watch": "parcel watch src/manifest.json --config @parcel/config-webextension --public-url /",
    "clean": "rm -rf dist .parcel-cache"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.80",
    "@mlc-ai/web-runtime": "^0.23.0-dev1",
    "idb": "^8.0.0"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.12.0",
    "@types/chrome": "^0.0.260",
    "buffer": "^6.0.3",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "typescript": "^5.3.0"
  }
}
